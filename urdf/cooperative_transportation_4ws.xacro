<link name="v1_front_right_wheel_1">
  <inertial>
    <origin xyz="1.779885572261719e-07 2.540692008445866e-07 -3.1158354346638717e-07" rpy="0 0 0"/>
    <mass value="${sim_wheel_mass}"/>
    <inertia ixx="${wheel_inertia_coef*0.113544}" iyy="${wheel_inertia_coef*0.222011}" izz="${wheel_inertia_coef*0.113544}" ixy="0.0" iyz="0.0" ixz="0.0"/>
  </inertial>
  <visual>
    <origin xyz="-2.050031 0.34248 -0.110215" rpy="0 0 0"/>
    <geometry>
      <mesh filename="package://cooperative_transportation_4ws_description/meshes/new_wheel_1.stl" scale="0.001 0.001 0.001"/>
    </geometry>
    <material name="_"/>
  </visual>
  <collision>
    <origin xyz="0 0 0" rpy="1.57 0 0"/>
    <geometry>
      <cylinder radius="0.153" length="0.05"/>
    </geometry>

    <surface>
      <friction>
        <ode>
          <mu>${wheel_surface_mu1}</mu>
          <mu2>${wheel_surface_mu2}</mu2>
          <!-- fdir1: mu が作用する第1方向。ここでは「進行方向」に合わせる -->
          <!-- タイヤ回転軸が +Y なので、進行方向はローカルX としておくのが分かりやすい -->
          <fdir1>1 0 0</fdir1>
          <!-- 強すぎるスティッキングを避けるためのスリップ（力依存） -->
          <slip1>0.001</slip1>   <!-- rolling -->
          <slip2>0.0</slip2>     <!-- lateral（基本0でOK、必要なら微小） -->
        </ode>
      </friction>
      <contact>
        <ode>
          <kp>${wheel_kp}</kp>
          <kd>${wheel_kd}</kd>
          <minDepth>${wheel_minDepth}</minDepth>
          <maxVel>${wheel_maxVel}</maxVel>
        </ode>
      </contact>
    </surface>
  </collision>
</link>

