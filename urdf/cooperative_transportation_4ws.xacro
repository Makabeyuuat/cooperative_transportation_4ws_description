<?xml version="1.0" ?>
<robot name="cooperative_transportation_4ws" xmlns:xacro="http://www.ros.org/wiki/xacro">

<xacro:property name="PI" value="3.1415926535897932384626433832795028841971" />
<xacro:property name="gravitational_acceleration" value="9.8" />
<xacro:property name="vehicle1_Yaw" value="0" />
<!-- <xacro:property name="vehicle2_Yaw" value="${-PI/8}" />
<xacro:property name="vehicle3_Yaw" value="${PI/6}" /> -->
<xacro:property name="vehicle2_Yaw" value="${-PI/8}" />
<xacro:property name="vehicle3_Yaw" value="${PI/6}" />


<!--各リンクの質量と慣性をfusionから変換 -->
<!-- 荷台 -->
<xacro:property name="fusion_carrier_mass" value="2052.2363345901408" /> 
<xacro:property name="sim_carrier_mass" value="100" /> 
<xacro:property name="carrier_inertia_coef" value="${sim_carrier_mass/fusion_carrier_mass}" /> 
<!-- -->
<xacro:property name="fusion_vehicle_body_mass" value="2113.272311120373" /> 
<xacro:property name="sim_body_mass" value="100" /> 
<xacro:property name="vehicle_inertia_coef" value="${sim_body_mass/fusion_vehicle_body_mass}" /> 

<!-- 車輪 -->
<xacro:property name="fusion_wheel_mass" value="${sim_wheel_mass}" /> 
<xacro:property name="sim_wheel_mass" value="8.0" /> 
<xacro:property name="wheel_inertia_coef" value="${sim_wheel_mass/fusion_wheel_mass}" /> 
<xacro:property name="wheel_joint_damping" value="0.1" />
<xacro:property name="wheel_joint_mu" value="0.001" />
<xacro:property name="wheel_joint_friction" value="${wheel_joint_mu * 0.0}" /> <!-- 横方向に垂直抗力は生まれないため0.0 -->




<xacro:property name="carrier_mass" value="2052.2363345901408" /> 
<xacro:property name="linkage_point_mass" value="16.301253040579397" /> 
<xacro:property name="vehicle_body_mass" value="2113.272311120373" /> 
<xacro:property name="sliding_mechanism_mass" value="36.58325703660448" /> 

<!-- 摺動機構 -->
<xacro:property name="sliding_mechanism_damping" value="${(0.5*2*sqrt(100000*(carrier_inertia_coef/3 + linkage_point_mass)))/5.0}" /> <!-- ばね定数：1000~10000, 割合：50%-->
<xacro:property name="sliding_mechanism_mu" value="0.3" />
<xacro:property name="sliding_mechanism_friction" value="${sliding_mechanism_mu*gravitational_acceleration*(carrier_inertia_coef/3 + linkage_point_mass)}" />
<!-- ステアリング -->
<xacro:property name="steering_damping" value="5.0" />
<xacro:property name="steering_mu" value="0.01" />
<xacro:property name="steering_friction" value="${steering_mu * gravitational_acceleration * 0.25*(carrier_inertia_coef/3 + linkage_point_mass + sliding_mechanism_mass + sim_body_mass)}" />
<!-- 摺動機構に乗っかている回転関節 -->
<xacro:property name="rotational_joint_damping" value="0.02" />
<xacro:property name="rotational_joint_mu" value="0.001" />
<xacro:property name="rotational_joint_friction" value="${rotational_joint_mu * gravitational_acceleration * carrier_inertia_coef/3}" />

<!-- wheelの接地面の摩擦などを定義 -->
<xacro:property name="wheel_surface_mu1" value="0.4"/>
<xacro:property name="wheel_surface_mu2" value="1.0" />
<xacro:property name="wheel_kp" value="2e4" /> 
<xacro:property name="wheel_kd" value="5.0" />
<xacro:property name="wheel_minDepth" value="0.01" />
<xacro:property name="wheel_maxVel" value="0.5" />

<xacro:include filename="$(find cooperative_transportation_4ws_description)/urdf/materials.xacro" />
<xacro:include filename="$(find cooperative_transportation_4ws_description)/urdf/cooperative_transportation_4ws.trans" />
<xacro:include filename="$(find cooperative_transportation_4ws_description)/urdf/cooperative_transportation_4ws.gazebo" />

